  "license": "AGPL-3.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
